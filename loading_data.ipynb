    "# Loading arff dataset\n",
    "import arff\n",
    "import pandas as pd\n",
    "\n",
    "def loadDEFT2015():\n",
    "    df = pd.DataFrame(columns=['text', 'emotion'])\n",
    "    data = list(arff.load('Corpus.de.tweets.en.fran√ßais/DEFT2015-arff/Train2.2.arff'))\n",
    "\n",
    "    for i in range(len(data)):\n",
    "        item = data[i]\n",
    "        df.loc[i] = [item[0], item[1]]\n",
    "\n",
    "    return df"
   ]
